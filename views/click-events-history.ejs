<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css">
  <title>History</title>
</head>
<body>
  <div class="container">
    <h1 style="margin-top: 2em;">History</h1>
    <hr>
    <table>
      <thead>
        <tr>
          <th>Time</th>
          <th>Name</th>
          <th>Email</th>
        </tr>
      </thead>
      <tbody>
        <% events.forEach(event => { %>
          <tr>
            <td><%= event.time %></td>
            <td><%= event.Name %></td>
            <td><%= event.Email %></td>
          </tr>
        <% }) %>
      </tbody>
    </table>
  </div>
</body>
</html>